---
import Exp from "./Exp.astro"

const exps = [
  { role: "Freelance Developer", company: "SelfEmploy", year: "Present"},
  { role: "Frontend Engineer", company: "TechX", year: 2023},
]
---

<section class="mt-28">
  <h2 class=" font-medium">Experiences</h2>
  <div class="mt-4 flex flex-col divide-y divide-zinc-200">
    {
      exps.map(exp => (
        <Exp role={exp.role} company={exp.company} year={exp.year}/>
      ))
    }
  </div>
</section>
