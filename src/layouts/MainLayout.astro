---
import Layout from "./Layout.astro"
import Header from "@components/Header.astro";
import Footer from "@components/Footer.astro";

type Props = {
  title: string;
  description: string;
  date?: string;
};

const props = Astro.props;
---

<Layout title={props.title} description={props.description} >
    <Header {...props}/>
    <slot/>
    <Footer/>
</Layout>